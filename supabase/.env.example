# FAB Backend - Environment Variables
# Copy this file to .env and fill in your values.
# For Supabase Edge Functions, set these via: supabase secrets set KEY=value

# Supabase (auto-populated in Edge Functions)
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Browserless.io (web scraping)
BROWSERLESS_URL=https://production-sfo.browserless.io
BROWSERLESS_TOKEN=your_browserless_token

# Encryption key for provider credentials (generate with: openssl rand -base64 32)
ENCRYPTION_KEY=your_32_byte_base64_encoded_key

# Resend (transactional email)
RESEND_API_KEY=re_your_resend_api_key
RESEND_FROM_EMAIL=FAB <notifications@yourdomain.gr>
